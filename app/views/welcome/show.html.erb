<h1>Activity Finder</h1>


<%= form_with url: "/search", method: :get, local: true do |form| %>
  <%= form.label :num_participants, "Number of Participants" %>
  <%= form.number_field :num_participants %>
  <%= form.submit "Find Activities" %>
<% end %>

<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %>